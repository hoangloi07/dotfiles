# BEGIN_KITTY_THEME
# Nightfox
include current-theme.conf
# END_KITTY_THEME

font_size 16
window_padding_width 8
enable_audio_bell no
confirm_os_window_close 0
cursor_trail 1
linux_display_server x11
shell zsh
# background_opacity 0.97


# BEGIN_KITTY_FONTS
font_family      family='Iosevka Nerd Font' features=+ss03
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
